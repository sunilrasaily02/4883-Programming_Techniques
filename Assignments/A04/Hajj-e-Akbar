
#include <iostream>

using namespace std;

int main()
{
    string str;
    int count=0;
    while(cin>>str)
    {
        if(str=="*")
        break;
        count++;
        
        cout<<"Case "<<count<<": ";
        if(str=="Hajj")
        cout<<"Hajj-e-Akbar"<<endl;
        else
        cout<<"Hajj-e-Asghar"<<endl;
        
        
        
    }

    return 0;
}
